{
  "name": "ngx-utils",
  "$schema": "/node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ngx-utils/src",
  "prefix": "hc",
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "configurations": {
        "production": {
          "tsConfig": "libs/ngx-utils/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ngx-utils/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "generatePackageJson": true,
      "updateBuildableProjectDepsInPackageJson": true
    },
    "test": {
      "executor": "@angular/build:karma",
      "options": {
        "tsConfig": "libs/ngx-utils/tsconfig.spec.json",
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ]
      }
    }
  }
}
